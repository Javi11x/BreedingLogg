@page
@model BreedingLogg.Pages.Usuarios.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Usuario[0].NombreUsuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usuario[0].ApellidosUsuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usuario[0].UsuarioUsuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usuario[0].ContraseñaUsuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usuario[0].CorreoUsuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usuario[0].NivelUsuario)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Usuario) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombreUsuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ApellidosUsuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UsuarioUsuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ContraseñaUsuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CorreoUsuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NivelUsuario)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.UsuarioId">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.UsuarioId">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.UsuarioId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
